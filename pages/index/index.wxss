/**index.wxss**/
.userinfo {
  display: flex;
  flex-direction: row;
  align-items: center;
  background-color: #bad;
  width: 100%;
  height: 230rpx;
  margin-top: 0.1rpx;
}
.people{
  display: flex;
  flex-direction: row;
  align-items: center;
  background-color: white;
  width: 100%;
  height: 160rpx;
  margin-top: 8rpx;
  justify-content: space-between;
}
.people-else{
  display: flex;
  flex-direction: row;
  align-items: center;
  background-color: white;
  margin-left: 20rpx;
  margin-right: 20rpx;
  width: 100%;
  height: 100rpx;
  margin-top: 17rpx;
  margin-bottom: 17rpx;
  justify-content: space-between;
}
.serch{
  display: flex;
  flex-direction: row;
  align-items: center;
  background-color: white;
  width: 100%;
  height: 100rpx;
  margin-top: 8rpx;
  justify-content: space-between;
}
.serch-else{
  display: flex;
  flex-direction: row;
  align-items: center;
  background-color: white;
  margin-left: 20rpx;
  margin-right: 20rpx;
  width: 100%;
  height: 100rpx;
  margin-top: 17rpx;
  margin-bottom: 17rpx;
  justify-content: space-between;
}

.userinfo-avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}

.userinfo-nickname {
  padding-left: 8px;
  color:#eee;
}

